{
  "name": "nisruignite",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently 'npm:dev:*'",
    "build": "npm run build:server && npm run build:client",
    "start": "node server.js",
    "lint": "eslint . --ext=.ts",
    "dev:client": "node ace build:client --watch",
    "dev:server": "node ace serve --watch",
    "build:client": "node ace build:client --production",
    "build:server": "node ace build --production"
  },
  "eslintConfig": {
    "extends": [
      "plugin:adonis/typescriptApp"
    ]
  },
  "eslintIgnore": [
    "build"
  ],
  "devDependencies": {
    "@adonisjs/assembler": "^5.9.5",
    "@japa/preset-adonis": "^1.2.0",
    "@japa/runner": "^2.5.0",
    "@types/proxy-addr": "^2.0.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/source-map-support": "^0.5.6",
    "adonis-preset-ts": "^2.1.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.34.0",
    "eslint-plugin-adonis": "^2.1.1",
    "pino-pretty": "^9.3.0",
    "typescript": "~4.6",
    "youch": "^3.2.3",
    "youch-terminal": "^2.2.0"
  },
  "dependencies": {
    "@adonisjs/ally": "^4.1.5",
    "@adonisjs/core": "^5.9.0",
    "@adonisjs/i18n": "^1.5.0",
    "@adonisjs/lucid": "^18.3.0",
    "@adonisjs/repl": "^3.1.11",
    "@adonisjs/session": "^6.4.0",
    "@adonisjs/shield": "^7.1.0",
    "@adonisjs/view": "^6.2.0",
    "@microeinhundert/radonis": "^5.0.3",
    "@microeinhundert/radonis-server": "^5.0.3",
    "@microeinhundert/radonis-unocss": "^5.0.3",
    "luxon": "^3.2.1",
    "pg": "^8.9.0",
    "proxy-addr": "^2.0.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.21"
  }
}
